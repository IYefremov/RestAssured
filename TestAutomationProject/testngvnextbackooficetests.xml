<suite name="Cyberiansoft R360 ReconPro tests" verbose="10" configfailurepolicy="continue">
       <!--parallel="classes" thread-count="3"-->>
    <listeners>
        <listener class-name="com.cyberiansoft.test.vnextbo.utils.TestListenerAllure"/>
    </listeners>

  <test name="Back Office Test Cases" preserve-order="true">
    <classes>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOServicesPartsAndLaborBundleTestCases">
            <methods>
                <include name="verifyUserCanAddLaborPriceServiceWithUseLaborTimes"/>
                <include name="verifyUserCanAddPartPriceServiceWithCategorySubcategoryAndPartName"/>
                <include name="verifyUserCanChangeSequenceNumberForMoneyServiceOrder"/>
                <include name="verifyUserCanDeletePartPriceService"/>
                <include name="verifyUserCanEditLaborPriceService"/>
                <include name="verifyUserCanRestoreDeletedPartPriceService"/>
                <include name="verifyUserCannotAddLaborServiceWithClickedRejectIcon"/>
            </methods>
        </class>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOInvoiceDetailsTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOQuickNotesTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOServicesTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOOperationsInvoicesTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOMonitorTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOMonitorAdvancedSearchTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOMonitorSimpleSearchTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOHomePage"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOPartsManagementSearchTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOPartsManagementOrderDetailsTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBONewSmokeTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOHomePageAccessRightsTestCases"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOLoginTests"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.VNextBOForgotPasswordTests"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.Inspections.VNextBOInspectionsGeneralTests"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.Inspections.VNextBOInspectionsDetailsTests"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.Inspections.VNextBOInspectionsArchivingTests"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.Inspections.VNextBOInspectionsApproveTests"/>
        <class name="com.cyberiansoft.test.vnextbo.testcases.Inspections.VNextBOInspectionsSearchTests"/>
    </classes>
